# VITE/REACT SPA: Redirect all routes to index.html
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Nếu request không phải file hoặc thư mục thật thì chuyển hướng về index.html
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.html [L]
</IfModule>
